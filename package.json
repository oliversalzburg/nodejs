{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "yarn3-typescript-nodejs-esbuild",
  "license": "MIT",
  "author": "Oliver Salzburg <oliver.salzburg@gmail.com>",
  "type": "module",
  "scripts": {
    "build": "node build.js",
    "clean": "rm -rf ./output",
    "lint": "yarn run lint:all",
    "lint:all": "yarn run lint:eslint && yarn run lint:prettier && yarn run lint:tsc",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --noEmit",
    "test": "tsc && node $(yarn bin mocha) output/*.test.js",
    "test:coverage": "tsc && c8 --reporter=html-spa node $(yarn bin mocha) output/*.test.js"
  },
  "devDependencies": {
    "@eslint/js": "9.10.0",
    "@oliversalzburg/eslint-config": "1.1.0",
    "@oliversalzburg/js-utils": "0.1.1",
    "@types/chai": "4.3.19",
    "@types/eslint": "9.6.1",
    "@types/mocha": "10.0.8",
    "@types/node": "20.16.5",
    "c8": "10.1.2",
    "chai": "5.1.1",
    "esbuild": "0.23.1",
    "eslint": "9.10.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsdoc": "50.2.3",
    "eslint-plugin-tsdoc": "0.3.0",
    "globals": "15.9.0",
    "lint-staged": "15.2.10",
    "mocha": "10.7.3",
    "prettier": "3.3.3",
    "prettier-package-json": "2.8.0",
    "prettier-plugin-organize-imports": "4.0.0",
    "prettier-plugin-sh": "0.14.0",
    "typescript": "5.6.2",
    "typescript-eslint": "8.5.0"
  },
  "packageManager": "yarn@4.5.0"
}
